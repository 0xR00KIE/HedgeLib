    <Enum description="NAVITYPE_WISPON_L" value="5" />
    <Enum description="NAVITYPE_STEP_BOTH" value="6" />
    <Enum description="NAVITYPE_STEP_L" value="7" />
    <Enum description="NAVITYPE_STEP_R" value="8" />
    <Enum description="NAVITYPE_HORMING_ATTACK" value="9" />
    <Enum description="NAVITYPE_STOMPING" value="10" />
    <Enum description="NAVITYPE_SPIN_DASH" value="11" />
    <Enum description="NAVITYPE_GRIND_MOVE" value="12" />
    <Enum description="NAVITYPE_BADDY_CHANGE" value="13" />
    <Enum description="NAVITYPE_COUNT" value="14" />
