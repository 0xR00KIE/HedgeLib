  <NaviType type="byte" description="">
	<Enum description="NAVITYPE_NONE" value="0" />
    <Enum description="NAVITYPE_JUMP" value="1" />
    <Enum description="NAVITYPE_SLIDING" value="2" />
    <Enum description="NAVITYPE_BOOST" value="3" />
