<?xml version="1.0" encoding="utf-8"?>
<Template>
  <ShapeType type="byte" description="The shape of this volume.">
	<Enum description="Box" value="0" />
	<Enum description="Sphere" value="1" />
	<Enum description="Cylinder" value="2" />
  </ShapeType>
	
  <Width type="float" description="The width of this volume." />
  <Height type="float" description="The height of this volume." />
  <Depth type="float" description="The depth of this volume." />
	
  <HideIfInput type="bool" description="" />
  <CautionType type="byte" description="What type of symbol to display.">
	<Enum description="None" value="255" />
	<Enum description="Exclamation" value="0" />
	<Enum description="Quick Step" value="1" />
