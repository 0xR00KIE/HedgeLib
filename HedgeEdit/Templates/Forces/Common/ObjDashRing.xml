<?xml version="1.0" encoding="utf-8"?>
<Template>
  <RingType type="uint" default="0" description="The type of hoop.">
	<Enum value="0" description="Dash Hoop" />
	<Enum value="1" description="Rainbow Rings" />
	<Enum value="2" description="Number Type(?)" />
  </RingType>
  
  <OutOfControl type="float" default="0.5" description="How long the game locks your control for after coming in contact with this object." />
  <KeepVelocityDistance type="float" default="0.25" description="How long you keep your velocity gained from this object before gravity takes effect." />
  <Speed type="float" default="500" description="How much speed the Dash Hoop will send you off with." />
  
  <IsVisible type="bool" default="true" description="Whether the DashRing is visible or not." />
  <IsPositionConstant type="bool" default="true" description="" />
  <IsVelocityConstant type="bool" default="true" description="" />
  <DoesCauseSpin type="bool" default="false" description="Whether the DashRing causes you to play your spinning animation or not." />
  
  <Event0Targets type="forcesObjectList" description="" />
  <Event0Type type="byte" description="">
    <Enum description="EVENT_ON" value="0" />
    <Enum description="EVENT_OFF" value="1" />
  </Event0Type>
  <Event0Timer type="float" description="" />
  
  <Event1Targets type="forcesObjectList" description="" />
  <Event1Type type="byte" description="">
    <Enum description="EVENT_ON" value="0" />
    <Enum description="EVENT_OFF" value="1" />
  </Event1Type>
  <Event1Timer type="float" description="" />
  
  <Event2Targets type="forcesObjectList" description="" />
  <Event2Type type="byte" description="">
    <Enum description="EVENT_ON" value="0" />
    <Enum description="EVENT_OFF" value="1" />
  </Event2Type>
  <Event2Timer type="float" description="" />
  
  <Extra type="model" value="cmn_obj_rainbowring.model" condition="RingType == 1" />
  <Extra type="model" value="cmn_obj_dashring.model" condition="RingType == 0" />
  <Extra type="description" value="Floating hoops" />
  <Extra type="RawByteLength" length="120" />
</Template>